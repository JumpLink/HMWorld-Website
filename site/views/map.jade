extends layout

block content
	//- for (var l=0; l<layers.length; l++)
	- var l = 0
		.map(style="width:#{width*tilewidth}px;height:#{height*tileheight}px")
			- var count = 0
			- for (var y = 0; y<height; y++)
				- for (var x = 0; x<width; x++, count++)
					.tile(class="tid_#{tiles[count].t_id} ts_#{tiles[count].ts_id}")